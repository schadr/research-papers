	Pages = {481--494},
	Priority = {2},
	Title = {An empirical study of speed and communication in globally distributed software development},
	Url = {http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=1205177},
	Volume = {29},
	Year = {2003},
